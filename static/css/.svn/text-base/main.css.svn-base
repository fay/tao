html,body {
	font-family: Georgia, monospace;
	font-size: 12px;
}

ul li {
	list-style: none;
	margin-left: -38px;
}

a {
	color: #4AC0F2;
}

a img {
	vertical-align: middle;
	border: none;
}

.body {
	margin-left: auto;
	margin-right: auto;
	width: 92%;
}

.content {
	margin-left: auto;
	margin-right: auto;
}

.compatible {
	font-size: 10px;
	color: #FF7900;
}

.head h1 {
	margin: 0;
	padding: 0;
	color: #5BC236;
	font-size: 60px;
	font-family: 'Baskerville', Georgia, Times, serif;
	float: left;
}

.head a {
	text-decoration: none;
}

.entries {
	padding-top: 20px;
}

.intro .symbol {
	color: #aaa;
	float: left;
	font-size: 180px;
}

.intro .text {
	float: left;
}

.intro .text li {
	height: 50px;
}

.intro .text li span {
	color: #5BC236;
	font-size: 15px;
}

.intro {
	color: #B80028;
	font-size: 40px;
}

.intro2 {
	float: right;
}

.intro2 a {
	font-size: 31px;
	color: #1F242A;
	text-decoration: none;
	padding: 10px 10px 10px 10px;
	border-radius: 10px;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	-khtml-border-radius: 10px;
	background-color: #eee;
	border-radius: 10px;
}

.site {
	float: left;
	font-size: 50px;
	padding-top: 40px;
	padding-bottom: 40px;
	border-radius: 10px;
	-moz-border-radius: 10px;
	-webkit-border-radius: 10px;
	-khtml-border-radius: 10px;
	background-color: #eee;
	margin-right: 5px;
}

.group {
	float: left;
	width: 48%;
	padding-right: 5px;
}

.douban_news {
	float: left;
	width: 30%;
}

.site span {
	color: #eee;
}

.site a {
	color: #999;
	text-decoration: none;
}

.site a:hover,.site a:hover span {
	color: #4AC0F2;
}

.entry a.title {
	text-decoration: none;
	color: #80C31C;
}

.menu {
	float: right;
	padding-left: 50px;
	padding-top: 30px;
}

.error {
	color: #FF7900;
}

.nav {
	border-radius: 3px;
	-moz-border-radius: 3px;
	-webkit-border-radius: 3px;
	-khtml-border-radius: 3px;
	background-color: #eee;
	padding: 7px 0px 7px 0px;
}

.nav a {
	padding: 7px 8px 7px 8px;
	font-weight: bold;
	text-decoration: none;
}

.nav a:hover {
	background-color: #ddd;
}

.nav .left {
	float: left;
}

.nav .right {
	float: right;
}

.menu a,h3,label {
	font-weight: bold;
	font-size: 20px;
	color: #4AC0F2;
}

.entry .title {
	font-weight: bold;
	font-size: 20px;
}

.entry .time {
	padding-left: 14px;
	background: url(../images/clock_icon.png) no-repeat center left;
}

.entry .view {
	padding-left: 18px;
	background: url(../images/eye.png) no-repeat center left;
}

.info a {
	color: #B80028;
}

.entry span {
	font-size: 16px;
}

.none {
	clear: both;
}

div.codeblock {
	padding: 5px;
	border: 1px solid #CCC;
	background-color: #EEE;
}

input,label {
	font-size: 1.5em;
}

img {
	border: none;
}

h4 {
	color: #FF7900;
}

.explore,.home {
	padding-top: 10px;
	font-family: 微软雅黑;
}

.groups {
	float: left;
	width: 60%;
}

.entry { /*border-top: #666 1px dotted;*/
	color: #000;
	padding: 10px 5px 10px 5px;
	/*background: #D7EAEA none repeat scroll 0 0;*/
	text-shadow: 0px 1px 1px #fff;
}

.entry img {
	padding: 2px 2px 2px 2px;
	border: #eee 1px solid;
}

.entry .portrait {
	float: left;
	width: 60px;
}

.entry .text {
	float: left;
}

.entry .info {
	padding-top: 10px;
}

.main {
	float: left;
	width: 80%;
}

.sidebar {
	float: left;
	width: 20%;
}

.updated_users {
	
}

.douban {
	padding-left: 16px;
	background: url(../images/douban.ico) no-repeat center left;
}

.delicious {
	padding-left: 16px;
	background: url(http://delicious.com/favicon.ico) no-repeat center left;
}

.summary {
	padding-top: 5px;
	/*
	background: url(../images/quote.png) no-repeat top left;*/
}

a .watch,a:hover .unwatch {
	color: #999;
}

a:hover .watch,a .unwatch {
	color: #F4376C;
}

.google_profile {
	padding-left: 18px;
	font-size: 18px;
	background: url(../images/paw.png) no-repeat center left;
}

.rss {
	padding-left: 16px;
	background: url(../images/feed-icon-14x14.png) no-repeat center left;
}

.google_reader {
	padding-left: 16px;
	background: url(https://www.google.com/reader/ui/favicon.ico) no-repeat
		center left;
}

#highlight { /*background: #D7EAEA;*/
	text-shadow: 0px 1px 1px #fff;
	border-radius: 5px;
	-moz-border-radius: 5px;
	-webkit-border-radius: 5px;
	-khtml-border-radius: 5px;
	background-color: #eee;
	margin-right: 5px;
}

.footer {
	text-shadow: 0px 1px 1px #fff;
	margin-top: 10px;
	background-color: #E4F6F8;
}

.footer .metainfo {
	width: 40%;
	margin-left: auto;
	margin-right: auto;
}

.about {
	padding-left: 28px;
	background: url(../images/tshirt.png) no-repeat center left;
}

.blog {
	padding-left: 28px;
	background: url(../images/house.png) no-repeat center left;
}

.login {
	padding-left: 28px;
	background: url(../images/key.png) no-repeat center left;
}

.broadcast {
	padding-left: 28px;
	background: url(../images/70-tv.png) no-repeat center left;
}

.discovery {
	padding-left: 28px;
	background: url(../images/71-compass.png) no-repeat center left;
}

.friends {
	padding-left: 28px;
	background: url(../images/29-heart.png) no-repeat center left;
}

.footer p {
	color: #000;
	background: #aaa;
	text-shadow: 0px 1px 1px #fff;
	text-align: center;
	border-top: 10px solid white;
}

.footer .metainfo a {
	font-size: 20px;
	color: #E86850;
}

.footer .metainfo li {
	font-size: 25px;
}

.question {
	padding-left: 28px;
	background: url(../images/envelope.png) no-repeat center left;
}

.arch {
	padding-left: 28px;
	background: url(../images/20-gear2.png) no-repeat center left;
}

.footer .metainfo  ul {
	padding-left: 45px;
	padding-right: 10px;
	border-right: 1px dotted #666;
	float: left;
}
